    "@ipld/car": "^5.0.1",
    "@ipld/dag-cbor": "^8.0.0",
    "@ipld/schema": "^4.1.2",
    "@typescript-eslint/parser": "^4.33.0",
    "axios": "^1.2.1",
    "caip": "^1.1.0",
    "eslint": "^7.32.0",
    "ipfs-core": "0.14.3",
    "ipfs-core-types": "^0.13.0",
    "multiformats": "^10.0.2",
    "typescript": "^4.9.3",
    "web3.storage": "^4.4.0"
  }
